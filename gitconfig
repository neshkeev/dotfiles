[alias]
	st = status
	ada = add -A
	ad = add
	co = commit -s
	top = log -n 1
	top5 = log -n 5
	top10 = log -n 10
	di = diff
	coa = commit -s --amend
	coar = commit -s --amend -C HEAD
	dic = diff --cached
	rj = checkout --
	pom = push origin master
	sh = stash
	shp = stash pop
	adu = add -u
	adi = add -i
	ft = fetch origin --prune
	mom = merge origin/master
	cor = commit -s -C HEAD
	un = reset HEAD
	ls = diff-tree --no-commit-id --name-only -r
	lsh = diff-tree --no-commit-id --name-only -r HEAD
	br = branch
	ch = checkout
	dik = diff --check
	dick = diff --cached --check
	cldf = clean -df
	po = push origin
	m = checkout master
	rom = rebase origin/master
	me = merge
